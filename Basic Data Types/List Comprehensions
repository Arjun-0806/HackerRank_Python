if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
    Result=[[X, Y, Z] for X in range(x + 1) for Y in range(y + 1) for Z in range(z + 1) if X + Y + Z != n]

    print(Result)
    # or you can try this 
'''
Result = []

for x in range(0, X + 1):
    for y in range(0, Y + 1):
        for z in range(0, Z + 1):
            if (x + y + z) != n:
                Result.append([x, y, z])

print(Result)
'''
